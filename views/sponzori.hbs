<style>
    .contents {
        padding-top: 3.14159em;
    }

    .sponsors {
        display: grid;
        grid-template-columns: repeat(5, minmax(0, 1fr));
        grid-gap: 3.14159em;
    }

    .image-container {
        position: relative;
    }

    .image-container .image-sizer {
        padding-bottom: calc(0.86 * 100%);
        transition: padding-bottom .3s ease;
        will-change: padding-bottom;
    }

    .image-container .image-frame {
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background-repeat: no-repeat;
        background-position: center center;
        background-size: contain;
    }

    .sponsor {
        background: rgba(0, 0, 0, .3);
        border-radius: 18px;
        padding: 30px;
    }
</style>

<div class="sponsors">
    {{#each sponsors}}
        <div class="sponsor" title="{{ this.Name }}">
            <div class="image-container">
                <div class="image-sizer"></div>
                <div class="image-frame" style="background-image: url('{{ this.Src }}')"></div>
            </div>
        </div>
    {{/each}}
</div>
